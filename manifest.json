{
  "name": "acivity ",
  "version": "1.0",
  "description": "Force activate a background tab",
  "permissions": ["tabs","declarativeContent","activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://192.168.43.164:5500/"],
      "js": ["content.js"]
    }
  ],
  "page_action": { 
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }}
  ,
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}