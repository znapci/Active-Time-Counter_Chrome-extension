{
  "name": "acivity ",
  "version": "1.0",
  "description": "Force activate a background tab",
  "permissions": ["tabs","declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://192.168.43.164:5500/"],
      "js": ["content.js"]
    }
  ],
  "page_action": { 
    "default_popup": "popup.html",
    "default_icon": {
    "16": "images/active/icon.png",
    "32": "images/active/icon32.png",
    "48": "images/active/icon48.png",
    "128": "images/active/icon128.png"
    }}
  ,
  "icons": {
    "16": "images/active/icon.png",
    "32": "images/active/icon32.png",
    "48": "images/active/icon48.png",
    "128": "images/active/icon128.png"
  },
  "manifest_version": 2
}